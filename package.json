{
  "name": "rdme",
  "version": "9.0.0-next.11",
  "description": "ReadMe's official CLI and GitHub Action.",
  "license": "MIT",
  "author": "ReadMe <support@readme.io> (https://readme.com)",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "rdme": "bin/run.js"
  },
  "files": [
    "/bin/run*",
    "/dist",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "api",
    "apidoc",
    "apis",
    "documentation",
    "microservice",
    "oai",
    "oas",
    "openapi",
    "openapi document",
    "openapi initiative",
    "openapi spec",
    "openapi specification",
    "postman",
    "readme",
    "swagger"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/readmeio/rdme.git"
  },
  "bugs": {
    "url": "https://github.com/readmeio/rdme/issues"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "package.json"
  },
  "type": "module",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@oclif/core": "^3.19.3-dev.0",
    "@oclif/plugin-autocomplete": "^3.0.5",
    "@oclif/plugin-help": "^6.0.11",
    "@oclif/plugin-warn-if-update-available": "^3.0.9",
    "chalk": "^5.3.0",
    "ci-info": "^4.0.0",
    "configstore": "^6.0.0",
    "debug": "^4.3.3",
    "formdata-node": "^6.0.3",
    "gray-matter": "^4.0.1",
    "ignore": "^5.2.0",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "oas": "^24.0.0",
    "oas-normalize": "^11.0.1",
    "open": "^10.0.2",
    "ora": "^6.3.1",
    "parse-link-header": "^2.0.0",
    "pluralize": "^8.0.0",
    "prompts": "^2.4.2",
    "semver": "^7.5.3",
    "simple-git": "^3.19.1",
    "string-argv": "^0.3.1",
    "table": "^6.8.1",
    "tmp-promise": "^3.0.2",
    "toposort": "^2.0.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@readme/better-ajv-errors": "^1.5.0",
    "@readme/eslint-config": "^13.4.0",
    "@readme/oas-examples": "^5.10.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/configstore": "^6.0.0",
    "@types/debug": "^4.1.7",
    "@types/js-yaml": "^4.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/parse-link-header": "^2.0.0",
    "@types/pluralize": "^0.0.33",
    "@types/prompts": "^2.4.2",
    "@types/semver": "^7.3.12",
    "@types/toposort": "^2.0.7",
    "@types/validator": "^13.7.6",
    "@vitest/expect": "^1.1.0",
    "ajv": "^8.11.0",
    "alex": "^11.0.0",
    "eslint": "^8.47.0",
    "husky": "^9.0.10",
    "js-yaml": "^4.1.0",
    "knip": "^4.3.0",
    "msw": "^2.0.3",
    "nock": "^13.2.7",
    "oclif": "^4.4.13-dev.0",
    "openapi-types": "^12.1.3",
    "pkg": "^5.8.1",
    "prettier": "^3.0.2",
    "rollup": "^4.3.0",
    "ts-node": "^10.9.1",
    "type-fest": "^4.3.1",
    "typescript": "^5.1.6",
    "vitest": "^1.1.0"
  },
  "scripts": {
    "build": "tsc",
    "build:exe": "npm run build && rollup -c && pkg dist/rollup-output.cjs --target host --output exe/rdme",
    "debug": "ts-node src/cli.ts",
    "esbuild:cmds": "npx esbuild --format=cjs --target=node20 --platform=node --bundle --outfile=bundle-run.cjs bin/run.cjs",
    "esbuild:oclif": "npx esbuild --format=cjs --target=node20 --platform=node --bundle --outfile=bundle-cmds.cjs dist/index.js",
    "lint": "alex . && knip && npm run lint:ts && npm run prettier && npm run schemas:check",
    "lint:ts": "eslint . --ext .js,.ts",
    "prebuild": "rm -rf dist/ && rm -f src/package.json && ln package.json src/package.json",
    "prepack": "npm run build",
    "prepare": "husky",
    "pretest": "npm run build",
    "prettier": "prettier --check .",
    "schemas:check": "./bin/json-schema-store.js",
    "schemas:write": "./bin/json-schema-store.js --update",
    "test": "npm run test:multi && npm run test:single",
    "test:multi": "vitest run",
    "test:single": "vitest run --config vitest.single-threaded.config.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "scope-case": [
        2,
        "never",
        "upper-case"
      ]
    }
  },
  "oclif": {
    "bin": "rdme",
    "dirname": "rdme",
    "commands": {
      "strategy": "explicit",
      "target": "./dist/index.js",
      "identifier": "COMMANDS"
    },
    "topicSeparator": ":",
    "helpClass": "./dist/lib/help",
    "hooks": {
      "prerun": {
        "target": "./dist/index.js",
        "identifier": "prerun"
      },
      "createGHA": {
        "target": "./dist/index.js",
        "identifier": "createGHA"
      }
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "topics": {
      "categories": {
        "description": "List or create categories in your ReadMe developer hub."
      },
      "docs": {
        "description": "Sync or prune Guides pages in your ReadMe developer hub."
      },
      "openapi": {
        "description": "Manage your API definition. Supports OpenAPI, Swagger, Postman."
      },
      "versions": {
        "description": "Manage your documentation versions."
      }
    }
  },
  "prettier": "@readme/eslint-config/prettier"
}
